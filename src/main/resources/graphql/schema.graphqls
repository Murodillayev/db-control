type Query {
    getAllTodo : [TodoDto!]
    getTodo : TodoDto
}

type Mutation {
    createTodo(dto : TodoCreateDto!) : TodoDto!
    deleteTodo(id : ID!) : Boolean!
}

type TodoDto {
    id: ID!
    title : String!
    description: String
    completed: Boolean!
}

input TodoCreateDto {
    title : String!
    description: String
}